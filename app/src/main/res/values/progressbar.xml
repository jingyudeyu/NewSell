<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Horizontal_progressbar">
        <attr name="text_size" format="dimension" />
        <attr name="text_color" format="color" />
        <attr name="process_unreached_color" format="color" />
        <attr name="process_reached_color" format="color" />
        <attr name="process_unreached_height" format="dimension" />
        <attr name="process_reached_height" format="dimension" />
        <attr name="process_text_offset" format="dimension"/>
        <attr name="text_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="gone" value="1" />
        </attr>
    </declare-styleable>


</resources>